{
  "rewrites": [
    {
      "source": "/oauth-callback",
      "destination": "/oauth-callback.html"
    }
  ],
  "headers": [
    {
      "source": "/oauth-callback.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/images/glory-logo.png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

